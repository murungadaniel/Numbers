    <subsection xml:id="subsec-Multiplying-a-fraction-by-a-fraction">
        <title>Multiplying a fraction by a fraction</title>
        <!-- <activity>
            <statement>
                <p>
                    Activity for Multiplying a fraction by a fraction goes in here
                </p>
            </statement>
        </activity> -->
        <example>
            <statement>
                <p>
                    Work out: <m>\frac{3}{5} \times \frac{7}{9}</m>
                </p>
            </statement>
            <solution>
                <p>
                    Multiply the numerators and the denominators separately.
                </p>
                <p>
                    <md>
                        <mrow>\frac{3}{5} \times \frac{7}{9} \amp = \frac{3 \times 7}{5 \times 9}</mrow>
                        <mrow> \amp = \frac{21}{45} </mrow>
                    </md>
                </p>
                <p>
                    simplify the answer
                </p>
                <p>
                    <md>
                        <mrow>\frac{21}{45}  \amp = \frac{21 \div 3}{45 \div 3} </mrow>
                        <mrow> \amp = \frac{7}{15} </mrow>
                    </md>
                </p>
            </solution>
        </example>
        <exercise>
            <statement>
                <p>
                    Multiplying a fraction by a fraction
                </p>
            </statement>
        </exercise>
    </subsection>
    <subsection xml:id="subsec-Multiplying-a-fraction-by-a-mixed-number">
        <title>Multiplying a fraction by a mixed number</title>
        <!-- <activity>
            <statement>
                <p>
                    Activity for Multiplying a fraction by a mixed number goes here
                </p>
            </statement>
        </activity> -->
        <example>
            <statement>
                <p>
                    Work out: <m>\frac{2}{3} \times 2 \frac{1}{7} </m>
                </p>
            </statement>
            <solution>
                <p>
                    <ol marker="a)" cols="2">
                        <li>
                            <p>
                                Convert <m>2 \frac{1}{7}</m> to improper fraction.
                            </p>
                            <p>
                                <md>
                                    <mrow>2 \frac{1}{7} \amp = \frac{7 \times 2 +1}{7} </mrow>
                                    <mrow></mrow>
                                    <mrow> \amp = \frac{15}{7} </mrow>
                                </md>
                            </p>
                        </li>
                        <li>
                            <p>
                                Multiply the fractions.
                            </p>
                            <p>
                                <md>
                                    <mrow>\frac{2}{3} \times \frac{15}{7} \amp = \frac{2 \times 15}{3 \times 7}</mrow>
                                    <mrow> </mrow>
                                    <mrow> \amp = \frac{30}{21} </mrow>
                                </md>
                            </p>
                        </li>
                        <li>
                            <p>
                                Simplify
                            </p>
                            <p>
                                <md>
                                    <mrow>\frac{30 \div 3}{21 \div 3} \amp = \frac{10}{7}</mrow>
                                    <mrow></mrow>
                                    <mrow>\frac{10}{7} \amp = 1 \frac{3}{7} </mrow>
                                </md>
                            </p>
                        </li>
                    </ol>
                </p>
            </solution>
        </example>
