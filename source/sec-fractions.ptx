                                \fill[orange] (-2,0) arc[start angle=180, end angle=90, radius=2cm] -- (0,0) -- cycle;
                                \fill[orange] (0,0) -- (2,0) arc[start angle=0, end angle=90, radius=2cm] -- cycle;
                                \fill[orange] (0,0) -- (0,-2) arc[start angle=270, end angle=360, radius=2cm] -- cycle;
                                \draw[white, line width=2mm] (0,-2) -- (0,2);
